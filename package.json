{
	"name": "obsidian-better-hotkey",
	"version": "1.0.0",
	"description": "This is a sample plugin for Obsidian (https://obsidian.md)",
	"main": "main.js",
	"type": "module",
	"packageManager": "pnpm@10.28.2",
	"scripts": {
		"dev": "node esbuild.config.mjs",
		"build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
		"version": "node version-bump.mjs && git add manifest.json versions.json",
		"lint": "eslint .",
		"lint:fix": "eslint . --fix",
		"test": "vitest run",
		"test:watch": "vitest"
	},
	"keywords": [],
	"license": "0-BSD",
	"devDependencies": {
		"@eslint/js": "^9.39.2",
		"@types/node": "20.19.31",
		"esbuild": "^0.27.2",
		"eslint": "^9.39.2",
		"eslint-plugin-obsidianmd": "^0.1.9",
		"globals": "14.0.0",
		"jiti": "2.6.1",
		"jsdom": "^28.0.0",
		"tslib": "2.4.0",
		"typescript": "^5.9.3",
		"typescript-eslint": "^8.54.0",
		"vitest": "4.0.18"
	},
	"dependencies": {
		"obsidian": "latest"
	}
}